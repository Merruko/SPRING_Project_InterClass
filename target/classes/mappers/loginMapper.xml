<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">

	<!-- 로그인 -->
	<select id="selectUserById" resultType="com.spring.proj.login.user.CustomUserDetails">
	<![CDATA[
            SELECT
                *
            FROM
                i_member
            WHERE
                mId = #{logId}
        ]]>
	</select>
	
	
</mapper>